{
  "name": "app-frontend",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/app-frontend/src",
  "prefix": "app",
  "generators": {
    "@schematics/angular:component": {
      "style": "scss"
    }
  },
  "targets": {
    "build": {
      "executor": "@angular/build:application",
      "options": {
        "browser": "apps/app-frontend/src/main.ts",
        "tsConfig": "apps/app-frontend/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          {
            "glob": "**/*",
            "input": "apps/app-frontend/public"
          }
        ],
        "styles": ["apps/app-frontend/src/styles.scss"],
        "outputPath": "dist/temp-app",
        "define": {
          "process.env.AUTH_API_URL": "\"${AUTH_API_URL}\""
        }
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kB",
              "maximumError": "1MB"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "4kB",
              "maximumError": "8kB"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "optimization": false,
          "extractLicenses": false,
          "sourceMap": true
        }
      },
      "defaultConfiguration": "production",
      "outputs": ["{options.outputPath}"]
    },
    "serve": {
      "executor": "@angular/build:dev-server",
      "options": {
        "buildTarget": "app-frontend:build:development",
        "port": 4200,
        "host": "localhost"
      },
      "configurations": {
        "production": {
          "buildTarget": "app-frontend:build:production"
        },
        "development": {
          "buildTarget": "app-frontend:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular/build:extract-i18n"
    },
    "test": {
      "executor": "@angular/build:karma",
      "options": {
        "tsConfig": "apps/app-frontend/tsconfig.spec.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          {
            "glob": "**/*",
            "input": "apps/app-frontend/public"
          }
        ],
        "styles": ["apps/app-frontend/src/styles.scss"]
      }
    }
  }
}
